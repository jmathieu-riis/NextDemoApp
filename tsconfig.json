{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
  ],
  "exclude": [
    "node_modules",
    "scripts",
  ],
  "typedocOptions": {
    "entryPoints": [
      "pages",
      "node_modules/react-bootstrap/esm/Card.d.ts",
      "node_modules/react-bootstrap/esm/helpers.d.ts",
    ],
    "out": "docs",
    "name": "Next Demo",
    "readme": "README.md",
  }
}